            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.nio.file.Files
import java.nio.file.Paths
import java.nio.charset.StandardCharsets
import org.apache.jmeter.services.FileServer

// Folder to save files (absolute or relative to JMeter&apos;s /bin)
def folder = &quot;/Users/tegpam/Projects/JMeter-Projects/JMeter-APITest/log&quot;

//def userDir = System.getProperty(&quot;user.dir&quot;);
//log.info(&quot;JVM User Directory: &quot; + userDir);

// Ensure folder exists
def dir = new File(folder)
if (!dir.exists()) {
    dir.mkdirs()
}
// Timestamped filenames (or replace with &quot;latest_request.json&quot;)
def timestamp = new Date().format(&quot;yyyyMMdd_HHmmss&quot;)
def sampler = ctx.getCurrentSampler()
def samplerName = sampler.getName()
def threadName = ctx.getThread().getThreadName()
def reqFile = Paths.get(folder, &quot;request_${samplerName}_${timestamp}.json&quot;)


// Save request body (from vars or sampler)
def requestBody = sampler.getArguments().getArgument(0).getValue()
Files.write(reqFile, requestBody.getBytes(StandardCharsets.UTF_8))



log.info(&quot;✅ Saved request to ${reqFile}&quot;)</stringProp>
